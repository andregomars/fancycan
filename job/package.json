{
  "name": "@fancycan/fcjb",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npx tsc && node dist/app.js",
    "build": "npx tsc",
    "test": "rimraf dist && jest",
    "lint": "npx tslint -c tslint.json -p tsconfig.json",
    "watch-ts": "npx tsc -w",
    "serve-debug": "npx nodemon --inspect dist/app.js",
    "watch-debug": "npx concurrently -k -p \"[{name}]\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "debug": "npm run build && npm run watch-debug",
    "deploy": "npx tsc && pm2 delete fcrx && pm2 start --env production"
  },
  "devDependencies": {
    "@types/bson": "^1.0.11",
    "@types/config": "^0.0.34",
    "@types/jest": "^23.3.13",
    "@types/mongodb": "^3.1.19",
    "@types/node": "^10.12.18",
    "jest": "^23.6.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "config": "^3.0.1",
    "fancycan-common": "^1.0.35",
    "fancycan-model": "^1.0.21",
    "json-rules-engine": "^2.3.2",
    "mongodb": "^3.1.13",
    "rxios": "^1.1.2",
    "rxjs": "^6.4.0"
  }
}
