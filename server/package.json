{
  "name": "fc-server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node dist/app.js",
    "serve": "npx tsc && node dist/app.js",
    "build": "npx tsc",
    "tslint": "npx tslint -c tslint.json -p tsconfig.json",
    "watch-ts": "npx tsc -w",
    "serve-debug": "npx nodemon --inspect dist/app.js",
    "watch-debug": "npx concurrently -k -p \"[{name}]\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "debug": "npm run build && npm run watch-debug"
  },
  "dependencies": {
    "@akanass/rx-http-request": "^3.1.0",
    "@types/mqtt": "^2.5.0",
    "config": "^3.0.0",
    "mongodb": "^3.1.10",
    "mqtt": "^2.18.8",
    "rxjs": "^6.3.3",
    "shortid": "^2.2.14",
    "split-frames": "^2.4.1"
  },
  "devDependencies": {
    "@types/config": "^0.0.34",
    "@types/mongodb": "^3.1.14",
    "@types/node": "^10.12.9",
    "@types/shortid": "^0.0.29",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.6",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
