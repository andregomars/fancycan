{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "align": false,
        "class-name": true,
        "no-console": false,
        "trailing-comma": false,
        "quotemark": [
            true,
            "single"
        ],
        "max-line-length": false,
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false
    },
    "rulesDirectory": []
}